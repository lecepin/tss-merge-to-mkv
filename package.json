{
  "name": "tss-merge",
  "version": "0.1.0",
  "author": "lecepin <leping086@vip.qq.com>",
  "description": "ts 格式视频合并工具；也可以其他视频格式进行转换使用。",
  "dependencies": {
    "@ant-design/icons": "^4.0.6",
    "@babel/core": "7.6.0",
    "@svgr/webpack": "4.3.2",
    "antd": "^4.1.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "8.0.6",
    "babel-plugin-named-asset-import": "^0.3.4",
    "babel-preset-react-app": "^9.0.2",
    "camelcase": "^5.2.0",
    "case-sensitive-paths-webpack-plugin": "2.2.0",
    "cross-spawn": "^7.0.2",
    "css-loader": "2.1.1",
    "dotenv": "6.2.0",
    "dotenv-expand": "5.1.0",
    "file-loader": "3.0.1",
    "fs-extra": "7.0.1",
    "html-webpack-plugin": "4.0.0-beta.5",
    "identity-obj-proxy": "3.0.0",
    "is-wsl": "^1.1.0",
    "jest": "24.9.0",
    "jest-environment-jsdom-fourteen": "0.1.0",
    "jest-resolve": "24.9.0",
    "jest-watch-typeahead": "0.4.0",
    "lp-file-name-sort": "^1.0.3",
    "mini-css-extract-plugin": "0.8.0",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "pnp-webpack-plugin": "1.5.0",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-normalize": "7.0.1",
    "postcss-preset-env": "6.7.0",
    "postcss-safe-parser": "4.0.1",
    "react": "^16.10.2",
    "react-app-polyfill": "^1.0.4",
    "react-dev-utils": "^9.1.0",
    "react-dom": "^16.10.2",
    "resolve": "1.12.0",
    "resolve-url-loader": "3.1.0",
    "sass-loader": "7.2.0",
    "semver": "6.3.0",
    "style-loader": "1.0.0",
    "terser-webpack-plugin": "1.4.1",
    "ts-pnp": "1.1.4",
    "url-loader": "2.1.0",
    "webpack": "4.41.0",
    "webpack-dev-server": "3.2.1",
    "webpack-manifest-plugin": "2.1.1",
    "workbox-webpack-plugin": "4.3.1"
  },
  "main": "./dist/main.js",
  "homepage": "./",
  "scripts": {
    "start": "nf start -p 3000",
    "start-electron": "npm run build-electron && electron .",
    "start-web": "node scripts/start.js",
    "build-web": "node scripts/build.js",
    "build-electron": "webpack --config config/webpack-electron.config.js",
    "pack": "npm run build-web && npm run build-electron && electron-builder",
    "pack-nomodule": "npm run build-web && npm run build-electron && rm -rf node_modules && electron-builder",
    "pack-win": "npm run build-web && npm run build-electron && electron-builder -w",
    "pack-mac": "npm run build-web && npm run build-electron && electron-builder -m"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.6.3",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "electron": "6.0.11",
    "electron-builder": "^21.2.0",
    "foreman": "^3.0.1",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "webpack-cli": "^3.3.11"
  },
  "babel": {},
  "build": {
    "publish": "never",
    "productName": "TS视频合并工具",
    "appId": "com.tss_to_mkv.app",
    "directories": {
      "output": "packs"
    },
    "npmRebuild": false,
    "files": [
      "dist/**/*"
    ],
    "mac": {
      "icon": "dist/web/icon/icon.icns"
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ],
      "icon": "dist/web/icon/icon.ico"
    },
    "linux": {
      "icon": "dist/web/icon/icon.icns"
    },
    "extraResources": [
      "public"
    ]
  }
}
